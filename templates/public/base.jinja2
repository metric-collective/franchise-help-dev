{% import 'components/footer.jinja2' as footer %}

<!DOCTYPE html>
<html>
<head>

    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0, shrink-to-fit=no">
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-QWTKZyjpPEjISv5WaRU9OFeRpok6YctnYmDr5pNlyT2bRjXh0JMhjY6hW+ALEwIH" crossorigin="anonymous">
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/js/bootstrap.bundle.min.js" integrity="sha384-YvpcrYf0tY3lHB60NNkmXc5s9fDVZLESaAA55NDzOxhy9GkcIdslK1eN7N6jIeHz" crossorigin="anonymous"></script>
    <script src="https://code.jquery.com/jquery-3.6.0.min.js"></script>

    {% block metaTags %}{% endblock %}

    <title>{{ title }}</title>

    <link rel="stylesheet" href="{{ url_for('static', filename='css/custom-styling-angelo.css') }}">
    <link rel="stylesheet" href="{{ url_for('static', filename='css/custom-styling-charuka.css') }}">
    <link rel="stylesheet" href="{{ url_for('static', filename='css/custom-styling-dileepa.css') }}">
    <link rel="stylesheet" href="{{ url_for('static', filename='css/custom-styling-kushan.css') }}">
    <link rel="stylesheet" href="{{ url_for('static', filename='css/style.css') }}">

    <script rel="script" src="{{ url_for('static', filename='js/app.js') }}"></script>

</head>

<body>


<div class="">
    {% block universe %}

        {% if not noHeader %}
            {% include 'components/header.jinja2' %}
        {% endif %}

        <section class="content">
            {% block content %}
            {% endblock %}
        </section>

        {% include 'components/footer.jinja2' %}

    {% endblock %}


</div>

</body>

</html>
